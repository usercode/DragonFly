@inherits AssetMetadataComponent<PdfMetadata>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            Pages
        </BSLabel>
        <div class="col-sm-10">
            <FluentTextField Value="@Metadata.CountPages?.ToString()" ReadOnly="true" Appearance="FluentInputAppearance.Filled" />
        </div>
    </BSRow>
</div>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            PDF version
        </BSLabel>
        <div class="col-sm-10">
            <FluentTextField Value="@Metadata.PdfVersion?.ToString()" ReadOnly="true" Appearance="FluentInputAppearance.Filled" />
        </div>
    </BSRow>
</div>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            Encrypted
        </BSLabel>
        <div class="col-sm-10">
            <FluentTextField Value="@Metadata.IsEncrypted?.ToString()" ReadOnly="true" Appearance="FluentInputAppearance.Filled" />
        </div>
    </BSRow>
</div>
