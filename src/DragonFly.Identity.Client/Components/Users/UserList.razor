@inherits UserListBase

<div class="content-header">
<Toolbar Items="ToolbarItems"></Toolbar>
</div>
<div class="scroll-content">
<BSListGroup>
	@foreach(IdentityUser user in Users)
	{
			<a class="list-group-item list-group-item-action" @onclick="x => OpenItem(user)">
			<BSRow>
				<BSCol LG="5">
					@user.Username
				</BSCol>
				<BSCol LG="5">
					@user.Email
				</BSCol>
			</BSRow>
			</a>
	}
</BSListGroup>
</div>