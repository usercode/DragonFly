@inject IDragonFlyApi Api

<BSListGroup>
	@foreach(var m in Api.Module().Modules.OrderBy(x => x.Name))
	{
		<BSListGroupItem>
			<BSRow>
				<BSCol>
					<strong>@m.Name</strong>
				</BSCol>
				<BSCol>
					@m.Author
				</BSCol>
				<BSCol>
					@m.Version
				</BSCol>
			</BSRow>
		</BSListGroupItem>	
	}
</BSListGroup>

@code {

}
