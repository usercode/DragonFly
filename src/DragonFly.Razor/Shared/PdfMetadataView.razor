@inherits AssetMetadataComponent<PdfMetadata>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            Pages
        </BSLabel>
        <div class="col-sm-10">
            <input type="text" class="form-control" @bind-value="@Metadata.CountPages" readonly />
        </div>
    </BSRow>
</div>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            PDF version
        </BSLabel>
        <div class="col-sm-10">
            <input type="text" class="form-control" @bind-value="@Metadata.PdfVersion" readonly />
        </div>
    </BSRow>
</div>

<div class="form-group">
    <BSRow>
        <BSLabel class="col-sm-2 col-form-label">
            Encrypted
        </BSLabel>
        <div class="col-sm-10">
            <input type="text" class="form-control" @bind-value="@Metadata.IsEncrypted" readonly />
        </div>
    </BSRow>
</div>

